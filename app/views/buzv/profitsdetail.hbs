          <option value="1">服装</option>
          <option value="2">食品</option>
        </select>
      </span>

    </fieldset>
    <fieldset>
      <span class="span3">
        <label for="">发货时间
        </label>
        <input class="input-small" type="text">
        -
        <input class="input-small" type="text">
      </span>
      <span class="span3">
        <label for="">分润金额
        </label>
        <input class="input-small" type="text">
        -
        <input class="input-small" type="text">
      </span>
      <span class="span3">
        <a href="/admin/itemadd" class="btn btn-primary">查看详情(这个需要吗)</a>
        <button type="submit" class="btn btn-primary">筛选</button>
        &nbsp;
        <a href="#">取消</a>
      </span>
    </fieldset>

  </form>
</div>

